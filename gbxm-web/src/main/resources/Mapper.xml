<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="zx.soft.snd.web.dao.Dao">
<insert id="insertName"  parameterType="String">
      insert into twitterUserName(name) values(#{name});
</insert>

<insert id="insertToken"  parameterType="Token">
      insert into twitterToken(tokenkey,tokensecret) values(#{tokenkey},#{tokensecret});
</insert>
</mapper>